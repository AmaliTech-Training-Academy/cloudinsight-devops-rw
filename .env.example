# Sample Environment Variables
# These would be encrypted using the team encryption script

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/myapp
DATABASE_NAME=myapp_production
DATABASE_USER=myapp_user
DATABASE_PASSWORD=super_secure_password

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=redis_secure_password

# AWS Configuration
AWS_S3_BUCKET=myapp-production-assets
AWS_S3_REGION=us-west-2

# API Keys
JWT_SECRET=your_jwt_secret_here
API_KEY=your_api_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Frontend Environment Variables (NEXT_PUBLIC_*)
NEXT_PUBLIC_API_BASE_URL=https://api.myapp.com/v1
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project

# Other Configuration
LOG_LEVEL=info
DEBUG=false
NODE_ENV=production